{
    "enabled": "true",
    "basic_security": "true",
    "ping_limit_seconds": "1",
    "allow_ports": [
        {
            "port": "22",
            "protocol": "tcp"
        },
        {
            "port": "3128",
            "protocol": "tcp",
            "input_interface": "wlan0",
            "address": "bles_network",
            "netmask": "bles_netmask"
        },
        {
            "port": "80",
            "protocol": "tcp",
            "input_interface": "wlan0"

        },
        {
            "port": "123",
            "protocol": "tcp",
            "input_interface": "wlan0"
        },
        {
            "port": "123",
            "protocol": "udp",
            "input_interface": "wlan0"
        }
    ],
    "masquerade": [
        {
            "output_interface": "eth0",
            "address": "bles_network",
            "netmask": "bles_netmask"
        }
    ],
    "redirect_ports": [
        {
            "port": "80",
            "to_port": "3128",
            "input_interface": "wlan0",
            "address": "bles_network",
            "netmask": "bles_netmask",
            "protocol": "tcp"
        }
    ],
    "redirect_destination": [
        {
            "input_interface": "eth0",
            "protocol": "tcp",
            "destination": {
                "address": "ip_public",
                "port": "80"
            },
            "to_destination": {
                "address": "bles_address",
                "port": "80"
            }
        }
    ]
}