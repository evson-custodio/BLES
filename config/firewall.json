{
    "enabled": "true",
    "basic_security": "true",
    "ping_limit_seconds": "1",
    "allow_ports": [
        {
            "port": "22",
            "protocol": "tcp"
        },
        {
            "port": "3128",
            "protocol": "tcp",
            "input_interface": "wlan0"
        },
        {
            "port": "80",
            "protocol": "tcp",
            "input_interface": "wlan0"

        },
        {
            "port": "123",
            "protocol": "tcp",
            "input_interface": "wlan0"
        },
        {
            "port": "123",
            "protocol": "udp",
            "input_interface": "wlan0"
        }
    ],
    "masquerade": [
        {
            "output_interface": "eth0"
        }
    ],
    "redirect_ports": [
        {
            "port": "80",
            "to_port": "3128",
            "input_interface": "wlan0",
            "protocol": "tcp"
        }
    ]
}